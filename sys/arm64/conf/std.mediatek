options 	SOC_MEDIATEK

# Uart
device		uart_ns8250

# GPIO / PINCTRL
device mt7622_pinctrl
device gpio
device gpioiic

device iic
device iicbus
device iicbb

# eth switch
#device 		etherswitch
#device 		miibus
#device      mt7531_switch
#device 		mdio
#device		mt7622_eth

# Ethernet, BPF and bridge support
#device                 miibus
#device                 mdio
device          rt
device          bpf
device          if_bridge

# Ethernet switch
device		etherswitch             # Enable etherswitch support
device		mtkswitch

# MMC/SD/SDIO Card slot support
device		sdhci		# SD controller
device		mmc		# mmc/sd bus
device		mmcsd		# mmc/sd flash cards

options         FDT
# DTBs
makeoptions     MODULES_EXTRA+="dtb/mediatek"
